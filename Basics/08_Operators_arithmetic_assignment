#include <stdio.h>
#include <stdint.h>


int main(void)
{
	uint8_t bValue1 = 2;
	uint8_t bValue2 = 20;
	uint8_t bResult = 0;

	bResult = bValue1 + bValue2;
	bResult = bValue2 - bValue1;
	bResult = bValue1 * bValue2;
	bResult = bValue2 / bValue1;


	bResult = bValue2 % bValue1;
	bValue2 = bValue2 + 1;
	bResult = bValue2 % bValue1;


	bResult = bResult + bValue1;
	bResult +=bValue1;



	(void)bResult;
  return 0;
}
