#include <stdio.h>
#include <stdint.h>

int main(void)
{
	uint8_t bValue1 = 20;
	uint8_t bValue2 = 2;
	uint8_t bResult = 0;

	bResult = bValue1 + bValue2;
	bResult = bValue1 - bValue2;
	bResult = bValue1 * bValue2;
	bResult = bValue1 / bValue2;

	bResult = bValue1 % bValue2;
	bValue2 = bValue1 + 1;
	bResult = bValue1 % bValue2;

	(void)bResult;

  return 0;
}
